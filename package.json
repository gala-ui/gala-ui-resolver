  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gala-ui/gala-ui-resolver.git"
  },
  "keywords": [
    "gala",
    "ui"
  ],
  "author": "cavinHuang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gala-ui/gala-ui-resolver/issues"
  },
  "homepage": "https://github.com/gala-ui/gala-ui-resolver#readme",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "*",
    "debug": "^4.3.4",
    "esbuild": "^0.14.39",
    "esno": "^0.14.1",
    "rollup": "^2.74.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "unplugin-vue-components": "^0.19.3"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
