{
  "name": "@gala-ui/gala-ui-resolver",
  "version": "0.0.5",
  "description": "gala ui unplugin-vue-components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup && esno scripts/postbuild.ts",
    "dev": "tsup --watch",
